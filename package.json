{
  "name": "orcinus",
  "version": "0.1.4",
  "description": "Docker Swarm Compose Tools",
  "main": "cli.js",
  "scripts": {
    "pre-test": "node test/pre-test.js",
    "post-test": "node test/post-test.js",
    "test": "npm run pre-test && sleep 5 && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/test.js && npm run post-test"
  },
  "bin": {
    "orcinus": "./cli.js",
    "orcinus-dashboard": "./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anak10thn/orcinus.git"
  },
  "keywords": [
    "docker",
    "swarm",
    "compose",
    "tools",
    "utility"
  ],
  "author": "Ibnu Yahya <anak10thn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anak10thn/orcinus/issues"
  },
  "homepage": "https://github.com/anak10thn/orcinus#readme",
  "dependencies": {
    "base-64": "^0.1.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "express": "^4.15.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "should": "^11.2.1",
    "file-extension": "^3.1.2",
    "utf8": "^2.1.2",
    "yamljs": "^0.2.8"
  },
  "nexe": {
    "input": "cli.js",
    "output": "orcinus",
    "temp": "build/",
    "debug": false,
    "nodeMakeArgs": [
      "-j",
      "8"
    ],
    "runtime": {
      "framework": "node",
      "version": "5.10.0",
      "ignoreFlags": true
    }
  }
}
